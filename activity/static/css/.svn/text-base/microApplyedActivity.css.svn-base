body{
    background-image: url("/static/images/background_1.jpg");
    background-size: 100%;
    background-repeat: no-repeat;
}

#pageone{
    background-color: rgba(255, 255, 255, 0.35);
}
#latestHeader{
    border: none;
    background-color: transparent;
    border-bottom: 1px solid #cce698;
}
#latestHeader h1{
    margin-top: 3rem;
    margin-bottom: 3rem;
    font-size: 1.9rem;
    color: white;
}
.activityItem{
    border-bottom:1px solid #a7ca7d;
    overflow: hidden;
    margin: 1rem 3rem;
}
dt{
    font-size: 1.6rem;
    margin: 0;
    margin-bottom: 1rem;
}
dd{
    font-size: 1rem;
    margin-left: 2rem;
    line-height: 1.9;
}
/*.ui-content{
    padding-left: 0;
    padding-right: 0;
}*/
.ui-page-theme-a .ui-btn{
    font-size: 1rem;
    background-color: transparent;
    border-color: #355614;
    border-radius: 15px 15px 15px 15px;
    color:#a97518;
    width: 35%;
    margin-top: 0.5rem;
    margin-bottom: 1rem;
    padding: 0.3rem 0.5rem;
    float: right;
}
.ui-page-theme-a .ui-btn:hover{
    background-color: #355614;
    color: white;
}
.innerDiv{
    overflow-x: auto;
    width: 100%;
    height: 39rem;
}