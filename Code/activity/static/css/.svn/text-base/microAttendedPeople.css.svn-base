body{
    background-image: url("/static/images/background_1.jpg");
    background-size: 100%;
    background-repeat: no-repeat;
}

#latestHeader{
    background-color: transparent;
    border-color: #a7ca7d;
}
#latestHeader h1{
    margin-top: 3rem;
    margin-bottom: 3rem;
    font-size: 2rem;
    color: white;
}
#attendedActivity{
    background-color: white;
    padding-left: 2rem;
    padding-right: 2rem;
}
.activityItem{
    border-bottom: 1px solid #e2e2e2;
    padding-top: 1rem;
    padding-bottom: 1rem;
    margin-top: 1rem;
    color: #949494;
}
.name,.num{
    float: left;
    margin-right: 4%;
    margin-bottom: 1rem;
}
.onekey{
    overflow: hidden;
}
#attendedActivity .onekeyToSignIn,#attendedActivity .onekeyToSignOut{
    background-color: transparent;
    text-decoration: none;
    color: #a9751a;
    cursor: pointer;
    padding: 0.5rem;
    border: 1px solid #365715;
    border-radius: 3rem 3rem 3rem 3rem;
}
#attendedActivity .onekeyToSignIn{
    float: left;
}
#attendedActivity .onekeyToSignOut{
    float: right;
}
#attendedActivity .onekeyToSignIn:hover,#attendedActivity .onekeyToSignOut:hover{
    background-color: #365715;
    color: white;
}
.signingBtn{
    overflow: hidden;
    width: 100%;
}
.signingBtn a{
    display: inline-block;
    width: 2rem;
    height: 2rem;
    border-radius: 3rem 3rem 3rem 3rem;
    border: 1px solid #95dc4b;
    cursor: pointer;
    margin-bottom: 1rem;
}
.signOut{
    float: right;
}
.signingBtn a img{
    width: 100%;
}
#attendedActivity .signOut{
    border: none;
}
.alert {
    color: white;
}
a.confirmEnd{
    color:white;
    margin-top: 2rem;
}